
# config.mk is generated by configure
include config.mk
# calib.mk is made by ME
include calib.mk

ifneq ($(BUILD_MODE),kernel)
all:
	make -C src
	make -C util

ifeq ($(UNITTEST_MODE),y)
	make -C test
endif
clean:
	make -C src clean
	make -C util clean
ifeq ($(UNITTEST_MODE),y)
	make -C test clean
endif
	rm -f $(LIB_DIR)/*
else # build only kernel library
all:
	make -C src
clean:
	make -C src clean
endif
